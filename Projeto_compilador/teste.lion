@start
roar n as int;
roar linha as text;
roar i as int;
roar j as int;

hunt(n);

if n <= 0 then {
    roarout("Erro: nÃºmero deve ser maior que zero!");
} else {
    i = 0;
    while i < n strike {
        linha = "1";
        j = 1;
        while j < i strike {
            linha = linha + " " + (i * (i - 1)) / 2;
            j = j + 1;
        }
        roarout(linha);
        i = i + 1;
    }
}
@end
